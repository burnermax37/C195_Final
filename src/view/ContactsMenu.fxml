<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="403.0" prefWidth="477.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactsMenuController">
   <children>
      <VBox prefHeight="403.0" prefWidth="477.0">
         <children>
            <Label text="Contacts" />
            <TableView fx:id="contactsTable" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="contactIdColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="contactNameColumn" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="contactEmailColumn" prefWidth="75.0" text="Email" />
              </columns>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="contactScheduleButton" mnemonicParsing="false" onAction="#onActionViewSchedule" text="View Schedule" />
                  <Button mnemonicParsing="false" onAction="#onActionDisplayMain" text="Back" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
