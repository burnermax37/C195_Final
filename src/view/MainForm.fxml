   <children>
      <VBox alignment="CENTER" prefHeight="262.0" prefWidth="320.0" spacing="20.0">
         <children>
            <Label fx:id="welcomeLabel" text="&lt;welcomeLabel&gt;" />
            <Button mnemonicParsing="false" onAction="#onActionLoadAppointmentsMenu" text="Appointments Menu" />
            <Button mnemonicParsing="false" onAction="#onActionLoadCustomersMenu" text="Customers Menu" />
            <Button mnemonicParsing="false" onAction="#onActionLoadContactsMenu" text="Contacts Menu" />
         </children>
      </VBox>
   </children>
</AnchorPane>
